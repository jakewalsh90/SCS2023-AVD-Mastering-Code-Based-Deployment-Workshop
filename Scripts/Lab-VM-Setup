# Install Chocolatey
Set-ExecutionPolicy Bypass -Scope Process -Force; [System.Net.ServicePointManager]::SecurityProtocol = [System.Net.ServicePointManager]::SecurityProtocol -bor 3072; iex ((New-Object System.Net.WebClient).DownloadString('https://community.chocolatey.org/install.ps1'))
# Install Apps
choco install terraform --version 1.3.9 -y
choco install packer --version 1.8.5 -y
choco install azure-cli --version 2.45.0 -y
choco install az.powershell --version 9.4.0 -y
choco install vscode --version 1.82.3 -y
choco install remote-desktop-client --version 1.2.4582 -y